{
  "name": "rollup-plugin-import-map",
  "version": "1.0.0",
  "description": "Rollup plugin to apply import map mappings to a build",
  "type": "module",
  "main": "./dist/plugin.cjs",
  "exports": {
    "import": "./lib/plugin.js",
    "require": "./dist/plugin.cjs"
  },
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "test": "tap --no-esm test/*.js --no-coverage",
    "test:snapshot": "TAP_SNAPSHOT=1 tap --no-esm test/*.js --no-coverage",
    "lint": "eslint . --ext=js",
    "lint:fix": "eslint . --fix --ext=js",
    "prepare": "npm run -s build",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trygve-lie/rollup-plugin-import-map.git"
  },
  "keywords": [
    "Rollup",
    "Import",
    "Map",
    "ESM",
    "Modules",
    "ECMAScript"
  ],
  "author": {
    "name": "Trygve Lie",
    "email": "post@trygve-lie.com",
    "url": "https://github.com/trygve-lie"
  },
  "contributors": [
    {"name":"Keith Cirkel","url":"https://github.com/keithamus"},
    {"name":"Richard Walker","url":"https://github.com/digitalsadhu"}
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trygve-lie/rollup-plugin-import-map/issues"
  },
  "homepage": "https://github.com/trygve-lie/rollup-plugin-import-map#readme",
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.8",
    "@semantic-release/release-notes-generator": "9.0.1",
    "eslint": "7.14.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "rollup": "2.33.3",
    "semantic-release": "17.3.0",
    "tap": "14.11.0"
  }
}
